@model SIMS_CW.Models.category

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section title {
    Category Details
}
@section title_page {
    Category Details
}
<h3 class="headerw3" style="margin-bottom: 30px;">Category Details</h3>

<div>
    <hr/>
    <dl class="dl-horizontal">
        <dt>Category Name</dt>
        <dd>
            @Html.DisplayFor(model => model.category_name)
        </dd>

        <dt>Created At</dt>
        <dd>
            @Html.DisplayFor(model => model.created_at)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.category_id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
